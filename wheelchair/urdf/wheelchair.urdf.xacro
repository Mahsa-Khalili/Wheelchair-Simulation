<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="wheelchair">
<xacro:property name = "lower" value = "-1000"/>
<xacro:property name = "upper" value = "1000"/>
<xacro:macro name="default_coefficients_links">
    <kp>1e+10</kp>
    <kd>1</kd>
    <mu1>1</mu1>
    <mu2>1</mu2>
    <fdir1>0 0 0</fdir1>
    <maxVel>0.01</maxVel>
    <minDepth>0.0</minDepth>
    <max_contancts> 10 </max_contancts>
    <velocity_decay>
        <linear>0</linear>
        <angular>0</angular>
    </velocity_decay>
</xacro:macro>
<xacro:macro name="default_coefficients_joints">
   	<stopCfm>0</stopCfm>
  	<stopErp>0.2</stopErp>
</xacro:macro>


  	<link name='base_link'>
	    <visual>
	    	<oririn rpy= "0 -0 0" xyx = "-0.24919 -1.2e-05 -0.357491"/>
		    <geometry>
		    	<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Wheelchair-base_col.STL"/>
		    </geometry>
	    </visual>
	    <collision>
        	<oririn rpy= "0 -0 0" xyx = "-0.24919 -1.2e-05 -0.357491"/>
        	<geometry>
          		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Wheelchair-base_col.STL"/>
        	</geometry>
      	</collision>
	    <inertial>
        	<mass value="4.78178"/>
         	<inertia ixx="0.253724" ixy="7e-06" ixz="0.022948" iyy="0.218409" iyz="7e-06" izz="0.382313"/>
 	    </inertial>
  	</link>
  
  	<link name="Left-Caster-Wheel">
	  	<visual>
	  		<origin rpy = "0 -0 0" xyz = "-0.32661 -0.24669 -0.06"/>
	  		<geometry>
	  			<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Left-Caster-Wheel.STL"/>
	  		</geometry>
	  	</visual>
	  	<collision>
        	<origin rpy = "0 -0 0" xyz = "-0.32661 -0.24669 -0.06"/>
        		<geometry>
          			<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Left-Caster-Wheel_col.STL"/>
        		</geometry>
     	</collision>
	  	<inertial>
        	<mass value="0.149507"/>
         	<inertia ixx="0.000176" ixy="8e-06" ixz="0.0" iyy="0.000339" iyz="0.0" izz="0.000176"/>
 	    </inertial>
  	</link>

  	<link name="Left-Caster-Yaw">
	  	<visual>
		  	<origin rpy = "0 -0 0"  xyz = "-0.35956 -0.24291 -0.1517"/>
		  	<geometry>
		  		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Left-Caster-Yaw.STL"/>
		  	</geometry>
		</visual>
		<collision>
          	<origin rpy = "0 -0 0" xyz = "-0.35956 -0.24291 -0.1517"/>
          	<geometry>
            	<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Left-Caster-Yaw_col.STL"/>
          	</geometry>
      	</collision>
		<inertial>
        	<mass value="0.279084"/>
         	<inertia ixx="0.000442" ixy="1e-06" ixz="0.000123" iyy="0.000458" iyz="8e-06" izz="0.000182"/>
 	    </inertial>
 	</link>

  	<link name="Left-Wheel">
	  	<visual>
	  		<origin rpy  = "0 -0 0" xyz = "-0 -0.26808 -0.305"/>
	  		<geometry>
		  		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Left-Wheel.STL"/>
		  	</geometry>
	  	</visual>
	  	<collision>
        	<origin rpy  = "0 -0 0" xyz = "-0 -0.26808 -0.305"/>
        	<geometry>
          		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Left-Wheel_col.STL"/>
        	</geometry>
      	</collision>
	  	<inertial>
        	<mass value="2.91669"/>
         	<inertia ixx="0.112302" ixy="0" ixz="0.0" iyy="0.222779" iyz="0.0" izz="0.112302"/>
 	    </inertial>
  	</link>

  	<link name="Right-Caster-Wheel">
	  	<visual>
	  		<origin rpy = "0 -0 0" xyz = "-0.32661 0.24108 -0.06"/>
	  		<geometry>
	  			<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Right-Caster-Wheel.STL"/>
	  		</geometry>
	  	</visual>
	  	<collision>
        	<origin rpy  = "0 -0 0" xyz = "-0.32661 0.24108 -0.06"/>
        	<geometry>
          		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Right-Caster-Wheel_col.STL"/>
        	</geometry>
      	</collision>
	  	<inertial>
        	<mass value="0.149507"/>
         	<inertia ixx="0.000176" ixy="8e-06" ixz="0.0" iyy="0.000339" iyz="0.0" izz="0.000176"/>
 	    </inertial>
  	</link>

  	<link name="Right-Caster-Yaw">
	  	<visual>
		  	<origin rpy = "0 -0 0" xyz = "-0.35956 0.24291 -0.1517"/>
		  	<geometry>
		  		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Right-Caster-Yaw.STL"/>
		  	</geometry>
		</visual>
		<collision>
        	<origin rpy = "0 -0 0" xyz = "-0.35956 0.24291 -0.1517"/>
        	<geometry>
          		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Right-Caster-Yaw_col.STL"/>
        	</geometry>
     	</collision>
		<inertial>
	        <mass value="0.279085"/>
	         <inertia ixx="0.000442" ixy="1e-06" ixz="0.000123" iyy="0.000458" iyz="8e-06" izz="0.000182"/>
 	    </inertial>
  	</link>

  	<link name="Right-Wheel">
	  	<visual>
	  		<origin rpy  = "0 -0 0" xyz = "0 0.26808 -0.305"/>
	  		<geometry>
		  		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Right-Wheel.STL"/>
		  	</geometry>
	  	</visual>
	  	<collision>
        	<origin rpy  = "0 -0 0" xyz = "0 0.26808 -0.305"/>
        	<geometry>
          		<mesh scale=" 1 1 1" filename = "package://wheelchairbot/meshes/Right-Wheel_col.STL"/>
        	</geometry>
      	</collision>
	  	<inertial>
	        <mass value="2.91699"/>
	        <inertia ixx="0.112302" ixy="0" ixz="0" iyy="0.222779" iyz="0" izz="0.112302"/>
 	    </inertial>
  	</link>

	<joint name="Left-Caster-Yaw-Left-Caster-Wheel" type="revolute">
	   <parent link="Left-Caster-Yaw"/>
	   <child link="Left-Caster-Wheel"/>
	   <origin xyz="-0.04295 -0.00183 -0.0917"/>
	   <axis xyz="0 1 0"/>
	   <limit effort= "-1" lower = "${lower}" upper = "${upper}" velocity = "-1"/>
	</joint>

	<joint name="Wheelchair-base-Left-Caster-Yaw" type="revolute">
	   <parent link="base_link"/>
	   <child link="Left-Caster-Yaw"/>
	   <origin xyz="0.35956 0.24291 0.1517"/>
	   <axis xyz="0 0 -1"/>
	   <limit effort= "-1" lower = "${lower}" upper = "${upper}" velocity = "-1"/>
	</joint>

	<joint name="Wheelchair-base-Left-Wheel" type="revolute">
	   <parent link="base_link"/>
	   <child link="Left-Wheel"/>
	   <origin xyz="-0 0.26808 0.305"/>
	   <axis xyz="0 1 0"/>
	   <limit effort= "-1" lower = "${lower}" upper = "${upper}" velocity = "-1"/>
	</joint>

	<joint name="Right-Caster-Yaw-Right-Caster-Wheel" type="revolute">
	   <parent link="Right-Caster-Yaw"/>
	   <child link="Right-Caster-Wheel"/>
	   <origin xyz="-0.04295 0.00183 -0.0917"/>
	   <axis xyz="0 1 0"/>
	   <limit effort= "-1" lower = "${lower}" upper = "${upper}" velocity = "-1"/>
	</joint>

	<joint name="Wheelchair-base-Right-Caster-Yaw" type="revolute">
	   <parent link="base_link"/>
	   <child link="Right-Caster-Yaw"/>
	   <origin xyz="0.35956 -0.24291 0.1517"/>
	   <axis xyz="0 0 1"/>
	   <limit effort= "-1" lower = "${lower}" upper = "${upper}" velocity = "-1"/>
	</joint>

	<joint name="Wheelchair-base-Right-Wheel" type="revolute">
	   <parent link="base_link"/>
	   <child link="Right-Wheel"/>
	   <origin xyz="0 -0.26808 0.305"/>
	   <axis xyz="0 1 0"/>
	   <limit effort= "-1" lower = "${lower}" upper = "${upper}" velocity = "-1"/>
	</joint>
	
    <transmission name="Wheelchair-base-Left-Wheel-trans">
      	<type>transmission_interface/SimpleTransmission</type>
      	<actuator name="Wheelchair-base-Left-Wheel-motor">
        	<mechanicalReduction>1</mechanicalReduction>
     	</actuator>
      	<joint name="Wheelchair-base-Left-Wheel">
        	<hardwareInterface>VelocityJointInterface</hardwareInterface>
      	</joint>
    </transmission>

    <transmission name="Wheelchair-base-Right-Wheel-trans">
      	<type>transmission_interface/SimpleTransmission</type>
      	<actuator name="Wheelchair-base-Right-Wheel-motor">
        	<mechanicalReduction>1</mechanicalReduction>
      	</actuator>
      	<joint name="Wheelchair-base-Right-Wheel">
        	<hardwareInterface>VelocityJointInterface</hardwareInterface>
      	</joint>
    </transmission>


	<gazebo>
		<plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
			<leftJoint>Wheelchair-base-Left-Wheel</leftJoint>
			<rightJoint>Wheelchair-base-Right-Wheel</rightJoint>
			<robotBaseFrame>base_link</robotBaseFrame>
			<wheelSeparation>0.53616</wheelSeparation>
			<wheelDiameter>0.609</wheelDiameter>
			<publishWheelJointState>true</publishWheelJointState>
		</plugin>
		<plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
			<jointName>Left-Caster-Yaw-Left-Caster-Wheel, Wheelchair-base-Left-Caster-Yaw, Wheelchair-base-Left-Wheel, Right-Caster-Yaw-Right-Caster-Wheel, Wheelchair-base-Right-Caster-Yaw, Wheelchair-base-Right-Wheel</jointName>
		</plugin>
	</gazebo>

	<gazebo reference="base_link">
		<xacro:default_coefficients_links/>
	</gazebo>
	<gazebo reference = "Left-Caster-Yaw">
		<xacro:default_coefficients_links/>
	</gazebo>
	<gazebo reference="Left-Caster-Wheel">
		<xacro:default_coefficients_links/>
	</gazebo>
	<gazebo reference="Left-Wheel">
		<xacro:default_coefficients_links/>
		<gravity>true</gravity>
	    <sensor name="imu_sensor" type="imu">
	      	<always_on>true</always_on>
	      	<update_rate>100</update_rate>
	      	<visualize>true</visualize>
	      	<topic>__default_topic__</topic>
	      	<plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
	        	<topicName>imu_left</topicName>
	        	<bodyName>Left-Wheel</bodyName>
	        	<updateRateHZ>10.0</updateRateHZ>
	        	<gaussianNoise>0.0</gaussianNoise>
	        	<xyzOffset>0 0 0</xyzOffset>
	        	<rpyOffset>0 0 0</rpyOffset>
	        	<frameName>Left-Wheel</frameName>
	        	<initialOrientationAsReference>false</initialOrientationAsReference>
	      </plugin>
	      <pose>0 0 0 0 0 0</pose>
	    </sensor>
	</gazebo>
	<gazebo reference = "Right-Caster-Yaw">
		<xacro:default_coefficients_links/>
	</gazebo>
	<gazebo reference="Right-Caster-Wheel">
		<xacro:default_coefficients_links/>
	</gazebo>
	<gazebo reference="Right-Wheel">
		<xacro:default_coefficients_links/>
		<gravity>true</gravity>
	    <sensor name="imu_sensor" type="imu">
	      	<always_on>true</always_on>
	      	<update_rate>100</update_rate>
	      	<visualize>true</visualize>
	      	<topic>__default_topic__</topic>
	      	<plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
	        	<topicName>imu_right</topicName>
	        	<bodyName>Right-Wheel</bodyName>
	        	<updateRateHZ>10.0</updateRateHZ>
	        	<gaussianNoise>0.0</gaussianNoise>
	        	<xyzOffset>0 0 0</xyzOffset>
	        	<rpyOffset>0 0 0</rpyOffset>
	        	<frameName>Right-Wheel</frameName>
	        	<initialOrientationAsReference>false</initialOrientationAsReference>
	      </plugin>
	      <pose>0 0 0 0 0 0</pose>
	    </sensor>
	</gazebo>

	<gazebo reference = "Left-Caster-Yaw-Left-Caster-Wheel">
		<xacro:default_coefficients_joints/>
	</gazebo>

	<gazebo reference="Wheelchair-base-Left-Caster-Yaw">
		<xacro:default_coefficients_joints/>
	</gazebo>
	<gazebo reference="Wheelchair-base-Left-Wheel">
		<xacro:default_coefficients_joints/>
	</gazebo>
	<gazebo reference = "Right-Caster-Yaw-Right-Caster-Wheel">
		<xacro:default_coefficients_joints/>
	</gazebo>
	<gazebo reference="Wheelchair-base-Right-Caster-Yaw">
		<xacro:default_coefficients_joints/>
	</gazebo>
	<gazebo reference="Wheelchair-base-Right-Wheel">
		<xacro:default_coefficients_joints/>
	</gazebo>
</robot>